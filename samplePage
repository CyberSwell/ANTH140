function setCountyCode(countyCode, countyName) {
	//var myUrl = "StoreStateSearch?storeId=10051&langId=-1&catalogId=10051&ItemCode=000004249&SearchNow=on&ProdName=Crown Russe Vodka 80 Proof&county=" + countyCode;
	//window.location.href = myUrl;
	document.FromZipForm.countyName.value = countyName;
	document.FromZipForm.county.value = countyCode;
	document.FromZipForm.submit();
}

function showPageWithPageNum(pageNumber) {
	document.FromZipForm.pageNum.value = pageNumber;
	document.FromZipForm.submit();
}

function changeItemsPerPage(itmPerPage) {
	document.FromZipForm.perPage.value = itmPerPage;
	document.FromZipForm.submit();
}

function checkAvailability(storeNo){
 	document.CheckAvailabilityForm.storeNO.value = storeNo;
	document.CheckAvailabilityForm.action = "ProductDetailInStore";
    document.CheckAvailabilityForm.submit();
}

