<script type="text/javascript">
function clearOut(el){
 if (el.defaultValue==el.value) el.value = "";
 }
function scrollWindow()
{
window.scrollTo(0,0);
} 
function echeckFooter(str) {
        var at="@"
        var dot="."
        var lat=str.indexOf(at)
        var lstr=str.length
        var ldot=str.indexOf(dot)
        if (str.indexOf(at)==-1){
           alert("Invalid E-mail ID")
           return false
        }
        if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr){
           alert("Invalid E-mail ID")
           return false
        }
        if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr){
            alert("Invalid E-mail ID")
            return false
        }
         if (str.indexOf(at,(lat+1))!=-1){
            alert("Invalid E-mail ID")
            return false
         }
         if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){
            alert("Invalid E-mail ID")
            return false
         }
         if (str.indexOf(dot,(lat+2))==-1){
            alert("Invalid E-mail ID")
            return false
         }
         if (str.indexOf(" ")!=-1){
            alert("Invalid E-mail ID")
            return false
         }
         return true                    
    }

function ValidateFormFooter(){
    var emailID=document.frmSampleFooter.EmailAlertFooter;
    console.log("footer emailID : "+emailID);
    if ((emailID.value==null)||(emailID.value=="")){
        alert("Please Enter your Email ID");
        emailID.focus();
        return false;
    }
    if (echeckFooter(emailID.value)==false){
        emailID.focus()
        return false;
    }
    
    document.frmSampleFooter.submit();
    return true
 }

 </script>
