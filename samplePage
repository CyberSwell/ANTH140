  function findOtherStores(){
  	var storeId=10051;
	var languageId=-1;
	var catalogId=10051;
	var category1='';
	var zip_code1='';
	var city1='';
	var county1='';
	var storeNO1='';
	console.log('category1:'+category1);
	console.log('storeNO:'+storeNO1);
	console.log('county1:'+county1);
	console.log('zip_code1:'+zip_code1);
	console.log('city11111:'+zip_code1);
  
  $.ajax({
  	method: "POST",
  	url: "ShopStoreView",
  	data: { storeId: storeId, langId: languageId ,catalogId:catalogId,pageNum:"1",listSize:"15",category:category1,city:city1, zip_code:zip_code1, county:county1,storeNO:storeNO1}
  })
 .done(function( html ) {
  	$( "#storeList" ).html(html);
  	javascript:$('#storeList').dialog( 'open');
  });
    
  }

  $( document ).ready(function() {
  	$( "#storeList" ).dialog({width: 900,top:120,modal: true,autoOpen: false});
  });
</script>
