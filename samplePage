<script language="javascript"><!--
// Added for Auto Complete
function performSearch(searchString) {
while(searchString!=null&&searchString.indexOf('&')>-1){
    searchString = searchString.replace("&","+%26+");
}
var nodeLevel = 3;    // Default to Product name
    if(searchString.length < 3) {
        resetResultDisplay();
        return;
    }
// Check if Part Number is entered for Searching
    if(isNumeric(searchString)) {
        nodeLevel = 2;
    }else{
        nodeLevel = 3;
    }
    if(window.ActiveXObject){
        xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");   //Old versions of Internet Explorer (IE5 and IE6) uses an ActiveX Object
    }else{
        xmlhttp = new XMLHttpRequest(); //All modern browsers (IE7+, Firefox, Chrome, Safari, and Opera) have a built-in XMLHttpRequest object.
    }

xmlhttp.onreadystatechange=function()
 {
  if (xmlhttp.readyState==4 && xmlhttp.status==200)  
    {
        updateDom(xmlhttp.responseText, nodeLevel);
    }else{
        resetResultDisplay();
    }
 }
 if(searchString.length < 3){ //If search string is empty, reset the result display box.
    resetResultDisplay();
 }else{
    var contents=document.getElementById("searchResultsDivId");
    contents.style.width = '320px';
    if(nodeLevel == 3){
        var temp = "";
        for(var i = 0; i < searchString.length; i++){
        //    if((searchString.charAt(i) >= 'A' && searchString.charAt(i) <= 'Z')
        //        ||(searchString.charAt(i) >= 'a' && searchString.charAt(i) <= 'z'))
            if((searchString.charAt(i) >= 'A' && searchString.charAt(i) <= 'Z')
                ||(searchString.charAt(i) >= 'a' && searchString.charAt(i) <= 'z') || (searchString.charAt(i) == ' '))
            {
                temp = temp + searchString.charAt(i);
                temp = temp.toLowerCase();
            }
        }
        searchString = temp;
    }
    xmlhttp.open("GET","/webapp/wcs/stores/WineandSpirits/ShoppingArea/CatalogSection/SearchSubsection/AutoComplete.jsp?keyWord=" + escape(searchString) + "&nodeLevel=" + nodeLevel , true);
    /* End of modification */
    xmlhttp.send();
}
}

var t;
var tempString = "";

function waitForSearch(searchStr){
    resetResultDisplay(); 
    clearTimeout(t);
    tempString = "";
    if(searchStr.length >= 3) {
        tempString = searchStr;
        t=setTimeout("callPerformSearch()", 700);
    }
}

function callPerformSearch(){
    performSearch(tempString);
}


function validateSearchSubmit(){
	var searchTermVal = document.getElementById("SearchKeyWord").value;
	searchTermVal = searchTermVal.toLowerCase();
	searchTermVal = searchTermVal.replace(/^\s\s*/, '').replace(/\s\s*$/, '');
	searchTermVal = searchTermVal.replace(/[`~!@#$%^&*()_|+\-=?;:'",.<>\{\}\[\]\\\/]/gi, '');
	var my_string = new String("SEARCH");
	var searchCheck = searchTermVal.substr(0,6);
	var pad = "000000000";
		if(searchTermVal != my_string && searchCheck != "search" && searchTermVal != '""' && searchTermVal != '' && searchTermVal != "search" && searchTermVal !="null" && searchTermVal!="Search by Name or Product Code" && searchTermVal != "''" ){
			var searchTerm = document.getElementById("SearchKeyWord").value;
       	    searchTerm = searchTerm.replace(/[^a-zA-Z0-9-,.$\u2044$_\s]/g, "");
       	    if(!isNaN(searchTerm)){
       	    	if(searchTerm.length > '3'){
       	//    		searchTerm = '0000'+searchTerm;
       	    		searchTerm = pad.substring(0, pad.length - searchTerm.length) + searchTerm;
       	    	}
       	    }
       	    document.forms["CatalogSearchForm"].SearchKeyWord.value = searchTerm;
       	    document.forms["CatalogSearchForm"].searchTerm.value = searchTerm;
			document.forms["CatalogSearchForm"].submit();
       	   
		}
		else{
			document.getElementById("SearchKeyWord").value =  "Search by Name or Product Code";
			document.getElementById("SearchKeyWord").style.color = 'red';
		  return false;
		}
}


function updateDom(XML, nodeLevel) {    

    var contents=document.getElementById("searchResultsDivId");
    contents.innerHTML="";
    contents.style.border="1px solid #A5ACB2";
    contents.style.width = '357px';
    contents.innerHTML = XML;
}

function resetResultDisplay(){
    var contents=document.getElementById("searchResultsDivId");
    contents.innerHTML="";
    contents.style.border = '2px';
    contents.style.width = '0px';
    contents.style.height="0px";    
    return;  
}

function isNumeric(sText) {
    var validChars = "0123456789.";
    var isNumber = true;
    var char;
    
    for(i=0; i<sText.length && isNumber==true; i++) {
        char = sText.charAt(i);
        if(validChars.indexOf(char) == -1) {
            isNumber = false;
        }
    }
    return isNumber;
}

function clearSearchSampleText() {
    if (document.getElementById("SearchKeyWord").value == "Search by Name or Product Code") {
        document.getElementById("SearchKeyWord").value = "";
    }
}

function onPressEnterKeySearch(myfield,e)
{
    var keycode;
    if (window.event) keycode = window.event.keyCode;
    else if (e) keycode = e.which;
    else return true;
    if (keycode == 13)
    {
       validateSearchSubmit();
        return false;
    }else return true;
}

